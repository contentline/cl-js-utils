{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": ["> 0.5% in RU", "ie 11", "not dead", "not op_mini all"]
      }
    ],
    "@babel/typescript"
  ],
  "plugins": [
    "@babel/proposal-class-properties",
    "@babel/proposal-object-rest-spread",
    [
      "@babel/plugin-transform-runtime",
      {
        "useESModules": true
      }
    ]
  ]
}
