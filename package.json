{
  "name": "cl-js-utils",
  "version": "0.0.1",
  "description": "Content Line JS utils",
  "main": "lib/lindex.js",
  "mainDir": "lib",
  "scripts": {
    "build": "babel src --out-dir lib",
    "postinstall": "yarn build",
    "format": "prettier --write"
  },
  "repository": {
    "type": "git",
    "url": "gitea@git.contentline.ru:CONTENT_LINE/cl-js-utils.git"
  },
  "keywords": [
    "JS",
    "utils"
  ],
  "author": "cudr",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.16.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "!lib/*.{js,json,babelrc}": [
      "yarn run format",
      "git add"
    ]
  }
}
